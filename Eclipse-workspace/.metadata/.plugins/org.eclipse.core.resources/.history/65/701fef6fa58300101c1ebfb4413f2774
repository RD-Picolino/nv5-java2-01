package br.com.senaisp.bauru.rodrigo.test;

import java.sql.SQLException;

import br.com.senaisp.bauru.rodrigo.classes.Produto;

public class ProdutoTest {

	public static void main(String[] args) {
		try {
			Produto prd = Produto.create("Produto xpto", 5.0, 15.20);
			System.out.println("Produto " + prd.getId());
			//Listando os produtos
			List<Produto> lista = Produto.listarProdutos();
			for(Produto prod: lista) {
				System.out.println();
			}
		} catch (SQLException e) {
			e.printStackTrace();
		}
	}

}
